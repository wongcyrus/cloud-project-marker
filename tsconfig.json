{
    "compilerOptions": {
      "target": "es2022",
      "strict": false,
      "preserveConstEnums": true,
      "sourceMap": false,
      "module":"NodeNext",
      "moduleResolution":"nodenext",
      "esModuleInterop": true, 
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,  
    },
    "include": ["cloudprojectmarker/**/*.ts", "cloudprojectmarker/**/*.js"],
    "exclude": [ "cloudprojectmarker/node_modules/*","node_modules/*"]
}

